{
	"name": "liveness-mod",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"cf-typegen": "wrangler types",
		"deploy": "npm run build && wrangler deploy",
		"dev": "react-router dev",
		"postinstall": "npm run cf-typegen",
		"preview": "npm run build && vite preview",
		"typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
		"test": "node --test",
		"d1:create": "wrangler d1 create app_db_liveness",
		"d1:migrate": "wrangler d1 execute app_db_liveness --file migrations/0001_users.sql",
		"d1:migrate:local": "wrangler d1 execute app_db_liveness --local --file migrations/0001_users.sql",
		"d1:list": "wrangler d1 list"
	},
	"dependencies": {
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-progress": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"aws4fetch": "^1.0.20",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"face-api.js": "^0.22.2",
		"isbot": "^5.1.27",
		"lucide-react": "^0.542.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.7.1",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.0.12",
		"@cloudflare/workers-types": "^4.20250204.0",
		"@react-router/dev": "^7.7.1",
		"@tailwindcss/vite": "^4.1.4",
		"@types/node": "^20",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"tailwindcss": "^4.1.4",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.8.3",
		"vite": "^6.3.3",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.34.0"
	}
}
